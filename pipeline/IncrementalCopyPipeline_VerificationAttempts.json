{
	"name": "IncrementalCopyPipeline_VerificationAttempts",
	"properties": {
		"activities": [
			{
				"name": "LookupLastChangeTrackingVersionActivity",
				"type": "Lookup",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "CosmosDbSqlApiSource",
						"query": "SELECT top 1 c.MaxTS FROM c order by c._ts desc",
						"preferredRegions": []
					},
					"dataset": {
						"referenceName": "ChangeTrackingVerificationDataset",
						"type": "DatasetReference"
					}
				}
			},
			{
				"name": "IncrementalCopyActivity",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "LookupLastChangeTrackingVersionActivity",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "CosmosDbSqlApiSource",
						"query": {
							"value": "SELECT c.datestamp, c.userId, c.rememberMeId, c.verified, c.verification.dob, c.verification.age, c.verification.previousVerification,c.userUpdated from c where c._ts>@{activity('LookupLastChangeTrackingVersionActivity').output.firstRow.MaxTS}",
							"type": "Expression"
						},
						"preferredRegions": []
					},
					"sink": {
						"type": "AzureSqlSink"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['datestamp']"
								},
								"sink": {
									"name": "DateOfAttempt",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['userId']"
								},
								"sink": {
									"name": "UserId",
									"type": "Int32"
								}
							},
							{
								"source": {
									"path": "$['rememberMeId']"
								},
								"sink": {
									"name": "YotiRememberMeId",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['verified']"
								},
								"sink": {
									"name": "Verified",
									"type": "Boolean"
								}
							},
							{
								"source": {
									"path": "$['dob']"
								},
								"sink": {
									"name": "DobMatch",
									"type": "Boolean"
								}
							},
							{
								"source": {
									"path": "$['age']"
								},
								"sink": {
									"name": "AgeMatch",
									"type": "Boolean"
								}
							},
							{
								"source": {
									"path": "$['previousVerification']"
								},
								"sink": {
									"name": "NotPreviouslyVerified",
									"type": "Boolean"
								}
							},
							{
								"source": {
									"path": "$['userUpdated']"
								},
								"sink": {
									"name": "UserServiceUpdated",
									"type": "Boolean"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "SourceVerificationAttemptsDataset",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "SinkVerificationDataset",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "UpdateMax",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "IncrementalCopyActivity",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "CosmosDbSqlApiSource",
						"query": {
							"value": "SELECT top 1 c._ts FROM c where c._ts>@{activity('LookupLastChangeTrackingVersionActivity').output.firstRow.MaxTS} order by c._ts desc",
							"type": "Expression"
						},
						"preferredRegions": []
					},
					"sink": {
						"type": "CosmosDbSqlApiSink",
						"writeBehavior": "insert"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['_ts']"
								},
								"sink": {
									"path": "$['MaxTS']"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "SourceVerificationAttemptsDataset",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "ChangeTrackingVerificationDataset",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}